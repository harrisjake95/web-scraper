import time
from selenium import webdriver
from selenium.webdriver.chrome.service import Service
from selenium.webdriver.common.by import By

# Set up the ChromeDriver service and options
chrome_driver_path = "/Users/jakeharris/Downloads/chromedriver_mac64/chromedriver"  # Replace with the actual path to your ChromeDriver executable
service = Service(chrome_driver_path)
options = webdriver.ChromeOptions()
options.add_argument("--headless")  # Run ChromeDriver in headless mode (without a visible browser window)

# Create a ChromeDriver instance
driver = webdriver.Chrome(service=service, options=options)

# Load the webpage
url = "https://www.amazon.co.uk/s?k=airpods&crid=1K3VJPIZFEY9B&sprefix=airpods%2Caps%2C276&ref=nb_sb_noss_2"
driver.get(url)

# Wait for the page to load
time.sleep(5)

# Locate all the elements with the specified path
elements = driver.find_elements(By.CSS_SELECTOR, "span[class='a-size-medium a-color-base a-text-normal']")

# Get the text attribute of each element and print them
for element in elements:
    print(element.text)

# Quit the driver and close the browser
driver.quit()
